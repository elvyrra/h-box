{button icon="pencil"
    class="btn-primary"
    title="{text key='h-box.rename-element-title'}"
    href="{uri action='h-box-rename-element' elementId='{$element->id}'}"
    target="dialog"
}


{button icon="times"
    class="btn-danger"
    title="{text key='h-box.delete-element-title'}"
    href="{uri action='h-box-delete-element' elementId='{$element->id}'}"
    target="dialog"
}